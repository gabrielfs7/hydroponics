<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="domain.factory.greenhouse.greenhouse_factory" class="GSoares\Hydroponics\Domain\Factory\Greenhouse\GreenhouseFactory">
            <argument type="service" id="infrastructure.datetime.datetime_provider"/>
        </service>

        <service id="domain.factory.tank.tank_factory" class="GSoares\Hydroponics\Domain\Factory\Tank\TankFactory">
            <argument type="service" id="infrastructure.datetime.datetime_provider"/>
        </service>

        <service id="domain.factory.system.system_factory" class="GSoares\Hydroponics\Domain\Factory\System\SystemFactory">
            <argument type="service" id="infrastructure.datetime.datetime_provider"/>
        </service>

        <service id="domain.factory.plant.plant_factory" class="GSoares\Hydroponics\Domain\Factory\Plant\PlantFactory">
            <argument type="service" id="infrastructure.datetime.datetime_provider"/>
        </service>
    </services>
</container>