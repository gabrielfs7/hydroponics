<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="domain.service.greenhouse.greenhouse_deleter" class="GSoares\Hydroponics\Domain\Service\Greenhouse\GreenhouseDeleter">
            <argument type="service" id="domain.repository.greenhouse.greenhouse_repository"/>
        </service>

        <service id="domain.service.tank.tank_deleter" class="GSoares\Hydroponics\Domain\Service\Tank\TankDeleter">
            <argument type="service" id="domain.repository.tank.tank_repository"/>
        </service>
        
        <service id="domain.service.system.system_deleter" class="GSoares\Hydroponics\Domain\Service\System\SystemDeleter">
            <argument type="service" id="domain.repository.system.system_repository"/>
        </service>

        <service id="domain.service.crops.crops_deleter" class="GSoares\Hydroponics\Domain\Service\Crops\CropsDeleter">
            <argument type="service" id="domain.repository.crops.crops_repository"/>
        </service>
        
        <service id="domain.service.plant.plant_deleter" class="GSoares\Hydroponics\Domain\Service\Plant\PlantDeleter">
            <argument type="service" id="domain.repository.plant.plant_repository"/>
        </service>
    </services>
</container>